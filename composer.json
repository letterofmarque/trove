{
    "name": "marque/trove",
    "description": "Core models, services, and contracts for Marque tracker platform",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Letter Of Marque Software",
            "email": "dan@lom.software",
            "homepage": "https://lom.software"
        }
    ],
    "support": {
        "issues": "https://github.com/letterofmarque/marque/issues",
        "source": "https://github.com/letterofmarque/marque"
    },
    "require": {
        "php": "^8.2",
        "illuminate/support": "^12.0",
        "illuminate/database": "^12.0",
        "illuminate/events": "^12.0"
    },
    "require-dev": {
        "orchestra/testbench": "^10.0",
        "pestphp/pest": "^4.0",
        "mockery/mockery": "^1.6"
    },
    "autoload": {
        "psr-4": {
            "Marque\\Trove\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Marque\\Trove\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Marque\\Trove\\TroveServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "pest"
    },
    "minimum-stability": "stable",
    "prefer-stable": true,
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        }
    }
}
